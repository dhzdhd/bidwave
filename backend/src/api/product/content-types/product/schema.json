{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "auction_end": {
      "type": "datetime",
      "required": true
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "number": {
      "type": "decimal",
      "required": true
    },
    "bid_price": {
      "type": "biginteger",
      "required": true
    },
    "description": {
      "type": "richtext"
    },
    "auction_start": {
      "type": "datetime",
      "required": true
    },
    "available": {
      "type": "boolean",
      "required": true
    },
    "bids": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bid.bid",
      "mappedBy": "product"
    }
  }
}
